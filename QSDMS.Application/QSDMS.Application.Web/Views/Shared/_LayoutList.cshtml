            //    alwaysVisible: !1
            //})
            //更改父页面标题
            window.parent.document.title = window.parent.document.title.split('-')[0] + "-" + document.title;
        })
    </script>
</head>
<body class="gray-bg" style="height:100%">
    <div class="wrapper wrapper-content">
        @RenderBody()
        @Html.AntiForgeryToken()
    </div>
    @RenderSection("Scripts", false)
